import java.util.Scanner; 
 
class Even extends Thread{ 	 
 	@Override 
 	public void run(){ 
 	int a = 3; 
 	int ans=a*a; 
 	System.out.println(ans); 
 	} 
} 
 
class odd extends Thread{ 	 
 	@Override 
 	public void run(){ 
int a=3; 
 	int ans=a*a*a; 
 	System.out.println(ans); 
 	} 
} 
 
class MultiThreading{ 	  	public static void main(String args[]) 
 	{ 
 	Scanner sc=new Scanner(System.in);  	new Even().Start();  	new Odd().Start(); 
 	} 
} 
