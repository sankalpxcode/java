class A 	 
{  
protected int num = 20; 
void welcome() 	 
{  
System.out.println("Welcome A"); 	 
}  
} 
 
class C extends A 	 
{  
void welcome(int a) 	 
{  
System.out.println("Welcome C: "+a); 	 
}  
} 
 
class B extends C 	 
{  
public static void main(String args[]) 	 
{  
B b1 = new B(); 	 b1.welcome(); 	 b1.welcome(10); 	 
System.out.println("Number: "+b1.num); 	 
}  
} 
 
