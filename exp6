package myPackage; 	 
 	public class MyClass 
 	{ 
 	public void getName(String aishu) 
 	{ 
 	System.out.println(aishu); 
 	} 
} 
 
import myPackage.MyClass; 
 
 	public class PrintName 
 	{ 
 	public static void main(String args[]) 
 	{ 
 	String name = "Hello"; 
 	MyClass obj=new MyClass();  	obj.getName(name); 
 	} 
} 
